<?xml version="1.0" encoding="UTF-8"?>

<bridges xmlns="urn:ieee:std:802.1Q:yang:ieee802-dot1q-bridge"
         xmlns:sfsg="urn:ieee:std:802.1Q:yang:ieee802-dot1q-stream-filters-gates"
         xmlns:psfp="urn:ieee:std:802.1Q:yang:ieee802-dot1q-psfp">
	<bridge>
        <name>switch</name>
        <address>00:01:83:fe:12:01</address>
        <bridge-type>provider-edge-bridge</bridge-type>
        <component>
            <name>swp0</name>
            <type>edge-relay-component</type>
            <sfsg:stream-filters>
                <sfsg:stream-filter-instance-table>
                    <sfsg:stream-filter-instance-id>1</sfsg:stream-filter-instance-id>
                    <sfsg:wildcard/>
                    <sfsg:priority-spec>two</sfsg:priority-spec>
                    <sfsg:stream-gate-ref>1</sfsg:stream-gate-ref>
                    <sfsg:filter-specification-list>
                        <sfsg:index>1</sfsg:index>
                        <sfsg:maximum-sdu-size>800</sfsg:maximum-sdu-size>
                        <sfsg:flow-meter-ref>68</sfsg:flow-meter-ref>
                        <sfsg:stream-blocked-due-to-oversize-frame-enabled>false</sfsg:stream-blocked-due-to-oversize-frame-enabled>
                    </sfsg:filter-specification-list>
                </sfsg:stream-filter-instance-table>
            </sfsg:stream-filters>
            <sfsg:stream-gates>
                <sfsg:stream-gate-instance-table>
                    <sfsg:stream-gate-instance-id>1</sfsg:stream-gate-instance-id>
                    <sfsg:gate-enable>true</sfsg:gate-enable>
                    <sfsg:admin-gate-states>open</sfsg:admin-gate-states>
                    <sfsg:admin-ipv>wildcard</sfsg:admin-ipv>
                    <psfp:admin-control-list-length>1</psfp:admin-control-list-length>
                    <psfp:admin-cycle-time>16000</psfp:admin-cycle-time>
                    <psfp:admin-control-list>
                        <psfp:index>0</psfp:index>
                        <psfp:operation-name>set-gate-and-ipv</psfp:operation-name>
                        <psfp:parameters>
                            <psfp:gate-state-value>open</psfp:gate-state-value>
                            <psfp:ipv-type>two</psfp:ipv-type>
                            <psfp:time-interval-value>8000</psfp:time-interval-value>
                        </psfp:parameters>
                    </psfp:admin-control-list>
                    <psfp:admin-control-list>
                        <psfp:index>1</psfp:index>
                        <psfp:operation-name>set-gate-and-ipv</psfp:operation-name>
                        <psfp:parameters>
                            <psfp:gate-state-value>closed</psfp:gate-state-value>
                            <psfp:ipv-type>two</psfp:ipv-type>
                            <psfp:time-interval-value>8000</psfp:time-interval-value>
                        </psfp:parameters>
                    </psfp:admin-control-list>
                    <sfsg:config-change>true</sfsg:config-change>
                </sfsg:stream-gate-instance-table>
            </sfsg:stream-gates>
            <psfp:flow-meters>
                <psfp:flow-meter-instance-table>
                    <psfp:flow-meter-instance-id>68</psfp:flow-meter-instance-id>
                    <psfp:committed-information-rate>100000000</psfp:committed-information-rate>
                    <psfp:committed-burst-size>4000000</psfp:committed-burst-size>
                    <psfp:excess-information-rate>4000000</psfp:excess-information-rate>
                    <psfp:excess-burst-size>100000000</psfp:excess-burst-size>
                    <psfp:coupling-flag>one</psfp:coupling-flag>
                    <psfp:color-mode>color-blind</psfp:color-mode>
                    <psfp:drop-on-yellow>1</psfp:drop-on-yellow>
                </psfp:flow-meter-instance-table>
            </psfp:flow-meters>
        </component>
	</bridge>
</bridges>
